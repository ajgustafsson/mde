<?xml version="1.0" encoding="ASCII"?>
<G2:MainSystem
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:G2="Group2"
    xsi:schemaLocation="Group2 engine.ecore">
  <workflows
      name="w1">
    <nodes xsi:type="G2:Task"
        name="task1"
        start="true"
        permission="//@permissions.0"
        transition="//@workflows.0/@nodes.1"
        Description="Detta ar den forsta tasken"/>
    <nodes xsi:type="G2:Simple"
        name="simple1"
        previousTasks="//@workflows.0/@nodes.0"
        task="//@workflows.0/@nodes.2"/>
    <nodes xsi:type="G2:Task"
        name="task2"
        end="true"
        permission="//@permissions.0"
        Description="Detta ar task2"
        data=""/>
  </workflows>
  <users groups="//@userGroups.0"
      name="Eva"/>
  <userGroups
      permissions="//@permissions.0"
      name="grupp"/>
  <permissions
      name="perm"/>
</G2:MainSystem>
