<?xml version="1.0" encoding="ASCII"?>
<G2:MainSystem
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:G2="Group2"
    xsi:schemaLocation="Group2 engine.ecore">
  <workflows
      name="w1">
    <nodes xsi:type="G2:Task"
        name="task1"
        start="true"
        permission="//@permissions.1"
        transition="//@workflows.0/@nodes.1"
        description="Detta ar den forsta tasken"/>
    <nodes xsi:type="G2:Simple"
        name="simple1"
        previousTasks="//@workflows.0/@nodes.0"
        task="//@workflows.0/@nodes.2"/>
    <nodes xsi:type="G2:Task"
        name="task2"
        permission="//@permissions.0"
        transition="//@workflows.0/@nodes.3"
        description="Detta ar task2"
        data=""/>
    <nodes xsi:type="G2:Split"
        name="parallelFlowMaddafaaaaakka"
        previousTasks="//@workflows.0/@nodes.2"
        tasks="//@workflows.0/@nodes.4 //@workflows.0/@nodes.5"/>
    <nodes xsi:type="G2:Task"
        name="p1"
        permission="//@permissions.0"
        transition="//@workflows.0/@nodes.6"
        description="Do p1"
        previousTasks="//@workflows.0/@nodes.2"/>
    <nodes xsi:type="G2:Task"
        name="p2"
        permission="//@permissions.0"
        transition="//@workflows.0/@nodes.6"
        description="Do p2"
        previousTasks="//@workflows.0/@nodes.2"/>
    <nodes xsi:type="G2:Merge"
        name="waitForParallels"
        previousTasks="//@workflows.0/@nodes.4 //@workflows.0/@nodes.5"
        task="//@workflows.0/@nodes.7"/>
    <nodes xsi:type="G2:Task"
        name="endMaddafakka"
        end="true"
        permission="//@permissions.1"
        description="klaaaaart!"
        previousTasks="//@workflows.0/@nodes.4 //@workflows.0/@nodes.5"/>
  </workflows>
  <users groups="//@userGroups.0"
      name="Eva"/>
  <userGroups
      permissions="//@permissions.0 //@permissions.1"
      name="grupp"/>
  <permissions
      name="perm"/>
  <permissions
      name="perm1"/>
</G2:MainSystem>
